<Application
    x:Class="TravelApplication.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:viewmodel="using:TravelApplication"
    RequestedTheme="Light"
    xmlns:Custom="using:TravelApplication.ViewModels">
    <Application.Resources >
        <Custom:ViewModel x:Key="MainDataViewModel"/>
        <Custom:ViewModel x:Key="TravelPack"/>
        <Custom:ViewModel x:Key="HealthTravel"/>
        <Custom:ViewModel x:Key="Adventure"/>
        <Custom:ViewModel x:Key="Family"/>
        <Custom:ViewModel x:Key="Cruises"/>
        <Custom:ViewModel x:Key="Wedding"/>
        <Custom:ViewModel x:Key="User"/>
    </Application.Resources>

</Application>
